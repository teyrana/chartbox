// Small Schema for a tile cache
// Defined in the Flatbuffer IDL.

namespace yggdrasil.node;

// struct Vector2f {
//   x:float;
//   y:float;
// }

table TileCache {
  x:float32;
  y:float32;
  width:float32;

  dimension:uint32;
  // represents a 2d dimension-by-dimension grid
  grid:[uint8];
}

root_type TileCache;